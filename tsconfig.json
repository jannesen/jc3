{
    "compilerOptions": {
        "module": "amd",
        "target": "es2018",
        "noErrorTruncation": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "useUnknownInCatchVariables": false,
        "strict": true,
        "preserveConstEnums": false,
        "noEmitHelpers": true,
        "removeComments": true,
        "declaration": true,
        "jsx": "react",
        "baseUrl": ".",
        "typeRoots": [],
        "paths": {
            "jc3/*": [
                "src/*"
            ]
        }
    },
    "include": [
        "src/*",
        "sample/*",
        "test/*",
        "tools/*"
    ]
}